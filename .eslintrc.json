{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-unused-vars": "off",
        "camelcase": "off",
        "new-cap": "off",
        "max-len": "off",
        "no-underscore-dangle": "off",
        "object-curly-newline": "off"
    }
}
